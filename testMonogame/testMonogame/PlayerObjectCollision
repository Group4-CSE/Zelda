public class PlayerObjectCollision
        {
            public PlayerObjectCollision()
            {
            }

            Rectangle playerRect;
            Rectangle objectRect;
            Boolean collision;

            const int height = 16;
            const int width = 16;

            public void playerEnemyDetection(IPlayer player, List<IObject> items, List<IObject> blocks)
            {
                //Detect player enemy collision

                playerRect = new Rectangle((int)player.X, (int)player.Y, width, height);

                foreach (IObject item in items)
                {
                    objectRect = item.getDestRect();
                    //playerRect = player.getDestRect();

                    //if they've collided
                    if (item.getDestRect().Intersects(playerRect))
                    {
                        //generate collision rectangle
                        collisionHandler(collisionRect, player, item);
                    }
                   
                   

                    
                    
                }

            }
            public void collisionHandler(Rectangle collisionRect, IPlayer player, IObject collided)
            {
                int x = collisionRect.Width;
                int y = collisionRect.Height;
                if (x > y)
                {
                    //if player is above the collided thing
                    if ((playerRect.Y + playerRect.Height < collided.Y) && (playerRect.Y + playerRect.Height > collided.Height / 2))
                    {
                        player.Y -= y;
                    }
                    //otherwise player is below object
                    else
                    {
                        player.Y += y;
                    }
                }
                else
                {
                    if ((playerRect.X + playerRect.Width < collided.X) && (playerRect.X + playerRect.Width > collided.Width / 2))
                    {
                        player.X -= x;
                    }
                    //otherwise player is below object
                    else
                    {
                        player.X += x;
                    }
                }
             }
